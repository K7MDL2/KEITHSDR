{
  "files.exclude": {
    "**/.git": true,
    "**/.lib": true,
    "**/.vsteensy/**": true,
    ".pio": true,
    ".vscode/.browse.c_cpp.db*": true,
    ".vscode/c_cpp_properties.json": true,
    ".vscode/launch.json": true,
    ".vscode/ipch": true,
    ".lib": true,
    ".*.zip": true
  },
  "C_Cpp.exclusionPolicy": "checkFilesAndFolders",
  "C_Cpp.errorSquiggles": "Enabled",
  "C_Cpp.default.includePath": [
    "C:\\ProgramFiles\\Arduino",
    "C:\\Program Files (x86)\\Arduino",
    "C:\\Users\\%USERPROFILE%\\Documents\\Arduino\\libraries",
    "C:\\Program Files (x86)\\Arduino\\hardware\\teensy\\avr\\libraries"
  ],
  "task.autoDetect": "off",
  "compiler-explorer.url": "http://localhost:10240",
  "compiler-explorer.compiler": "g54",
  "compiler-explorer.options": [
    "-D__IMXRT1062__ -DTEENSYDUINO=156",
    "-DF_CPU=816000000",
    "-DUSB_MIDI_AUDIO_SERIAL",
    "-DLAYOUT_US_ENGLISH",
    "-DARDUINO_TEENSY41",
    "-DARDUINO=10815",
    "-std=gnu++14 -fno-exceptions -fpermissive -fno-rtti -fno-threadsafe-statics -felide-constructors -Wno-error=narrowing",
    "-mthumb -mcpu=cortex-m7 -mfloat-abi=hard -mfpu=fpv5-d16",
    "-O2",
    "-g -Wall -ffunction-sections -fdata-sections -nostdlib",
    null
  ],
  "gnuGlobal.libraryPath": [
    "C:\\Program Files (x86)\\Arduino\\libraries"
  ],
  "C_Cpp.default.compilerPath": "",
  "C_Cpp.default.mergeConfigurations": true
}